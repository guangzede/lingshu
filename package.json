{
  "name": "lingshu-app",
  "version": "1.0.0",
  "private": true,
  "description": "A Taro + React + TypeScript project for the Douyin mini program '灵枢'.",
  "main": "src/app.ts",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "Less",
    "framework": "React"
  },
  "scripts": {
    "prepare": "husky",
    "new": "taro new",
    "gen:version": "node scripts/generate-version.js",
    "dev": "npm run build:h5 -- --watch",
    "dev:local": "cross-env API_MODE=local npm run build:h5 -- --watch",
    "dev:remote": "cross-env API_MODE=remote npm run build:h5 -- --watch",
    "devw": "npm run build:weapp -- --watch",
    "build:weapp": "npm run gen:version && taro build --type weapp && node fix-build.js",
    "build:swan": "npm run gen:version && taro build --type swan",
    "build:alipay": "npm run gen:version && taro build --type alipay",
    "build:tt": "npm run gen:version && taro build --type tt",
    "build:h5": "npm run gen:version && taro build --type h5",
    "build:rn": "npm run gen:version && taro build --type rn",
    "build:qq": "npm run gen:version && taro build --type qq",
    "build:jd": "npm run gen:version && taro build --type jd",
    "build:harmony-hybrid": "npm run gen:version && taro build --type harmony-hybrid",
    "dev:weapp": "npm run build:weapp -- --watch ",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:harmony-hybrid": "npm run build:harmony-hybrid -- --watch",
    "test:hexagram": "ts-node test-hexagram.ts"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.13.0",
    "@supabase/supabase-js": "^2.0.0",
    "@tarojs/components": "4.1.10",
    "@tarojs/helper": "4.1.10",
    "@tarojs/plugin-framework-react": "4.1.10",
    "@tarojs/plugin-platform-alipay": "4.1.10",
    "@tarojs/plugin-platform-h5": "4.1.10",
    "@tarojs/plugin-platform-harmony-hybrid": "4.1.10",
    "@tarojs/plugin-platform-jd": "4.1.10",
    "@tarojs/plugin-platform-qq": "4.1.10",
    "@tarojs/plugin-platform-swan": "4.1.10",
    "@tarojs/plugin-platform-tt": "4.1.10",
    "@tarojs/plugin-platform-weapp": "4.1.10",
    "@tarojs/react": "4.1.10",
    "@tarojs/runtime": "4.1.10",
    "@tarojs/shared": "4.1.10",
    "@tarojs/taro": "4.1.10",
    "@tarojs/webpack5-runner": "^4.1.10",
    "clsx": "^2.1.1",
    "framer-motion": "^12.29.0",
    "lunar-javascript": "^1.7.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.160.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-class-properties": "7.25.9",
    "@babel/preset-react": "^7.24.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@tarojs/cli": "4.1.10",
    "@tarojs/plugin-generator": "4.1.10",
    "@tarojs/vite-runner": "4.1.10",
    "@types/minimatch": "^5",
    "@types/react": "^18.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "babel-preset-taro": "4.1.10",
    "eslint": "^8.57.0",
    "eslint-config-taro": "4.1.10",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^9.1.7",
    "less": "^4.2.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "react-refresh": "^0.14.0",
    "stylelint": "^16.4.0",
    "stylelint-config-standard": "^38.0.0",
    "terser": "^5.30.4",
    "typescript": "^5.4.5",
    "vite": "^4.2.0",
    "webpack": "^5.91.0",
    "webpack-dev-server": "^4.15.2"
  }
}
